{
  "name": "hapi-template",
  "version": "1.0.0",
  "description": "hapi based template with dotenv, babel and testing with lab",
  "repository": "github:cdwills/hapi-template",
  "main": "start.js",
  "scripts": {
    "start": "node start.js",
    "dev": "nodemon -q -x npm start",
    "test": "lab -T node_modules/lab-babel -t 100",
    "test-cov-html": "lab -T node_modules/lab-babel -r html -o coverage.html"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "author": "cdwills",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.2",
    "dotenv": "^2.0.0",
    "good": "^6.6.0",
    "good-console": "^5.3.1",
    "hapi": "^13.0.0"
  },
  "devDependencies": {
    "code": "^2.1.0",
    "lab": "^10.0.0",
    "lab-babel": "^1.1.1",
    "nodemon": "^1.9.1"
  }
}
