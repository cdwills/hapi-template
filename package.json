{
  "name": "hapi-template",
  "version": "1.0.0",
  "description": "hapi based template with dotenv, babel and testing with lab",
  "main": "server.js",
  "scripts": {
    "start": "node start.js",
    "dev": "nodemon -q -x npm start",
    "test": "lab -T node_modules/lab-babel -t 100",
    "test-cov-html": "lab -T node_modules/lab-babel -r html -o coverage.html"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "author": "cdwills",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "dotenv": "^1.2.0",
    "good": "^6.4.0",
    "good-console": "^5.2.0",
    "hapi": "^12.0.0"
  },
  "devDependencies": {
    "code": "^2.1.0",
    "lab": "^8.0.1",
    "lab-babel": "^1.1.1",
    "nodemon": "^1.8.1"
  }
}
